# NobetCep Environment Variables
# Copy this file to .env and fill in the values

# ====================
# Pharmacy API
# ====================
# Leave empty to use mock provider
PHARMACY_API_BASE_URL=
PHARMACY_API_KEY=

# ====================
# LLM / AI Chat
# ====================
# Leave empty to use stub responses
LLM_API_KEY=
LLM_MODEL=gpt-4o-mini

# ====================
# App Settings
# ====================
# Set to "false" to use real APIs
USE_MOCK_PROVIDER=true

# Cache TTL in seconds (default: 24 hours)
CACHE_TTL=86400

# Rate limiting (requests per minute)
RATE_LIMIT_RPM=60
